{
  "name": "poly-crypto",
  "version": "1.0.0",
  "description": "Encrypt and decrypt data with AES-256 GCM; compatible with PHP's openssl_encrypt() and Python's PyCryptodome.",
  "main": "index.js",
  "scripts": {
    "dist": "npx babel --presets=@babel/preset-env node/src/* --out-file index.js",
    "test": "npx mocha node/tests/*.spec.js",
    "test:all": "./vendor/bin/kahlan --spec=php/tests && npm test && pytest",
    "test:php": "./vendor/bin/kahlan --spec=php/tests",
    "test:python": "pytest",
    "key:encrypt": "node ./node/cli/aes-key-encrypt.js",
    "pass:encrypt": "node ./node/cli/aes-pass-encrypt.js",
    "key:decrypt": "node ./node/cli/aes-key-decrypt.js",
    "pass:decrypt": "node ./node/cli/aes-pass-decrypt.js",
    "bcrypt:hash": "node ./node/cli/bcrypt-hash.js",
    "bcrypt:verify": "node ./node/cli/bcrypt-verify.js",
    "dig:md5": "node ./node/cli/digest.js md5",
    "dig:sha1": "node ./node/cli/digest.js sha1",
    "dig:sha256": "node ./node/cli/digest.js sha256",
    "dig:sha512": "node ./node/cli/digest.js sha512",
    "rand:bytes": "node ./node/cli/rand.js bytes",
    "rand:hex": "node ./node/cli/rand.js hex",
    "rand:slug": "node ./node/cli/rand.js slug",
    "rand:fax": "node ./node/cli/rand.js fax",
    "rand:string": "node ./node/cli/rand-string.js"
  },
  "keywords": [
    "encryption",
    "decryption",
    "encrypt",
    "decrypt",
    "AES",
    "AES-256",
    "AES256",
    "AES-256-GCM",
    "AES256GCM",
    "GCM",
    "bcrypt",
    "md5",
    "sha1",
    "sha256",
    "sha512",
    "slug",
    "symetric",
    "encryption",
    "cipher",
    "ciphertext",
    "crypto",
    "cryptography",
    "Node",
    "NodeJS",
    "node-forge",
    "PHP",
    "openssl",
    "Python",
    "PyCrypto",
    "PyCryptodome",
    "random",
    "urandom"
  ],
  "author": "Ken Snyder",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "node-forge": "^0.7.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "parcel": "^1.11.0",
    "prettier": "^1.16.1"
  }
}
